.hamburger
  cursor pointer
  display inline-block
  height 30px
  margin auto
  width 25px
  z-index 2
  position absolute
  left 0
  top 0
  @media only screen and (min-width: 390px)
    left 55px
  @media only screen and (min-width: xs)
    left 65px
    width 35px
  @media only screen and (min-width: navigationBreakPoint)
    display none