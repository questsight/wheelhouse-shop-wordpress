.questionnaire__button
  display flex
  justify-content center
  position: absolute;
  bottom: -120px;
  left: calc(50% - 62px);
  button
    background themeColor
    color: #fff;
    padding 10px 20px
    cursor pointer
    margin 15px 0
    border-radius 5px
    text-transform uppercase
    font-size 14px
    border none
    outline none